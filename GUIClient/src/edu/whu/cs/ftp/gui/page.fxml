            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
      <Separator layoutX="376.0" layoutY="165.0" orientation="VERTICAL" prefHeight="217.0" prefWidth="6.0" />
